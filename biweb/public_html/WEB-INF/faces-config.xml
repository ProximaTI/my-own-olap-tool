<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>br.com.bi.biwebBundle</base-name>
      <var>biwebBundle</var>
    </resource-bundle>
  </application>
  <navigation-rule>
    <from-view-id>/queryEdit.jspx</from-view-id>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>*</from-view-id>
    <navigation-case>
      <from-outcome>queryCad</from-outcome>
      <to-view-id>/queryCad.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>queryEdit</from-outcome>
      <to-view-id>/queryEdit.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>