<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>br.com.bi.biwebBundle</base-name>
      <var>biwebBundle</var>
    </resource-bundle>
  </application>
  <navigation-rule>
    <from-view-id>/queryEdit.jspx</from-view-id>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>queryCad</managed-bean-name>
    <managed-bean-class>br.com.bi.view.managedbeans.QueryCad</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>listener</property-name>
      <value>#{queryEdit}</value>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>queryEdit</managed-bean-name>
    <managed-bean-class>br.com.bi.view.managedbeans.QueryEdit</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>cubeMetadata</managed-bean-name>
    <managed-bean-class>br.com.bi.view.managedbeans.CubeMetadataBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>listener</property-name>
      <value>#{queryEdit}</value>
    </managed-property>
  </managed-bean>
  <navigation-rule>
    <from-view-id>*</from-view-id>
    <navigation-case>
      <from-outcome>queryCad</from-outcome>
      <to-view-id>/queryCad.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>queryEdit</from-outcome>
      <to-view-id>/queryEdit.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>