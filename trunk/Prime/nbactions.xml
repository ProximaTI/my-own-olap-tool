<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-Executar Testes Integração</actionName>
            <displayName>Executar Testes Integração</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
            </goals>
            <activatedProfiles>
                <activatedProfile>integration-tests</activatedProfile>
            </activatedProfiles>
        </action>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>package</goal>
            </goals>
            <properties>
                <netbeans.deploy>true</netbeans.deploy>
                <netbeans.deploy.clientUrlPart>/main.jsf</netbeans.deploy.clientUrlPart>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <goals>
                <goal>package</goal>
            </goals>
            <properties>
                <netbeans.deploy>true</netbeans.deploy>
                <netbeans.deploy.clientUrlPart>/main.jsf</netbeans.deploy.clientUrlPart>
                <netbeans.deploy.debugmode>true</netbeans.deploy.debugmode>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <goals>
                <goal>package</goal>
            </goals>
            <properties>
                <netbeans.deploy>true</netbeans.deploy>
                <netbeans.deploy.clientUrlPart>/main.jsf</netbeans.deploy.clientUrlPart>
                <netbeans.deploy.profilemode>true</netbeans.deploy.profilemode>
            </properties>
        </action>
    </actions>
